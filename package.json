{
  "name": "MiniHtmlExtractPlugin",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "ln -s ../test node_modules/@test  && ln -s ../src node_modules/@src",
    "start": "run-s clean build:production",
    "start:dev": "nodemon --exec 'run-s clean clear build:development' --watch ./ --ext js,ts,tsx --ignore ./build",
    "start:web": "webpack-dev-server -d --content-base ./build",
    "start:node": "nodemon --watch 'src/**/*.ts' --exec 'npm run clear && ts-node src/index.ts'",
    "build:development": "webpack --mode development",
    "build:production": "webpack --mode production",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "test:dev": "nodemon --watch ./ --ext ts --delay 0 --exec 'run-s clear test'",
    "clean": "rm -rf ./build && mkdir ./build",
    "clear": "printf \"\\33c\\e[3J\""
  },
  "author": "luics.xu@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/assert": "^1.5.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^8.0.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.8",
    "sass-loader": "^7.1.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
